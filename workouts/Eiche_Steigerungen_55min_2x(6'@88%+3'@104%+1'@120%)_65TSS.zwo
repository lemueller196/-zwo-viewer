            <textevent timeoffset="590" message="Straight into your tempo effort." />
        </Warmup>
        <SteadyState Duration="600" Power="0.88449997" pace="0" Cadence="95" />
        <SteadyState Duration="300" Power="0.55000001" pace="0">
